{
    "C_Cpp.default.compilerPath": "/opt/homebrew/bin/arm-none-eabi-gcc",
    "C_Cpp.default.cStandard": "c11",
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.intelliSenseMode": "gcc-arm",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/firmware/bk7258/include",
        "${workspaceFolder}/firmware/bk7258/libs/**",
        "${workspaceFolder}/firmware/bk7258/src/**",
        "/opt/homebrew/arm-none-eabi/include/**"
    ],
    "C_Cpp.default.defines": [
        "BK7258",
        "ARM_MATH_CM33",
        "CONFIG_SOC_BK7258",
        "__FPU_PRESENT=1U",
        "DEBUG=1"
    ],
    "files.associations": {
        "*.h": "c",
        "*.c": "c",
        "*.hpp": "cpp",
        "*.cpp": "cpp"
    },
    "C_Cpp.errorSquiggles": "enabled",
    "C_Cpp.autocompleteAddParentheses": true,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "search.exclude": {
        "**/build": true,
        "**/output": true,
        "**/.git": true,
        "**/node_modules": true
    },
    "cortex-debug.armToolchainPath": "/opt/homebrew/bin",
    "cortex-debug.JLinkGDBServerPath": "/Applications/SEGGER/JLink/JLinkGDBServerCL"
}